{
    "presets": ["next/babel"],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ],
      [
        "module-resolver",
        {
          "root": ["./"],
          "alias": {
            "components": "./components",
            "containers": "./containers",
            "contexts": "./contexts",
            "constants": "./constants",
            "pages": "./pages",
            "public": "./public",
            "api": "./api",
            "lib": "./lib",
            "utils": "./utils",
            "helpers": "./helpers"
          }
        }
      ]
    ]
  }